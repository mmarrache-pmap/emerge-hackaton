{
  "name": "emerge-hackaton",
  "version": "1.0.0",
  "description": "Effient Rebels were here",
  "main": "index.js",
  "scripts": {
    "clean:build": "rm -rf build && mkdir build",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "rm -rf build",
    "start": "webpack-dev-server --config ./config/webpack.config.dev.js --hot --inline --progress",
    "prebuild": "npm run clean:build && npm run build:html",
    "build": "SET NODE_ENV=production& NODE_ENV=production& webpack -p --config ./config/webpack.config.prod.js --progress",
    "lint": "eslint config src/js/**/*.js",
    "jscs": "jscs src/js/",
    "test": "mocha --compilers js:babel-core/register,css:./test/unit/cssNullCompiler.js --require ./test/unit/testHelper.js --recursive ./test/unit",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmarrache-pmap/emerge-hackaton.git"
  },
  "author": "Cyrus Walker <cwalker@processmap.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mmarrache-pmap/emerge-hackaton/issues"
  },
  "homepage": "https://github.com/mmarrache-pmap/emerge-hackaton#readme",
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "classnames": "^2.2.3",
    "history": "^2.0.2",
    "lodash": "^4.11.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.2.4",
    "redux": "^3.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^2.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.2",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.10",
