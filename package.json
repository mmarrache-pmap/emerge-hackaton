{
  "name": "emerge-hackathon",
  "version": "1.0.0",
  "author": "cwalker@processmap.com, mmarrache@processmap.com",
  "description": "eMerge Hackathon project",
  "private": false,
  "scripts": {
    "expr": "npm run build && node server.babel.js ",
    "start": "npm run build && webpack-dev-server --history-api-fallback --devtool eval-source-map --hot --inline --progress --colors",
    "build": "npm run clean && webpack --progress --colors && cp ./src/index.ejs views/.",
    "build:prod": "npm run clean && NODE_ENV=production webpack --progress --colors",
    "build-old": "NODE_ENV=production && npm run clean && webpack -p",
    "clean": "rimraf views",
    "lint": "eslint ./",
    "test": "mocha --require ./test/test_helper 'test/**/*.js'",
    "test:watch": "npm run test -- --watch",
    "git-update": "git update-index --again"
  },
  "precommit": {
    "run": "git-update",
    "silent": true
  },
